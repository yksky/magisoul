        <typeAlias alias="GcDbConfigInfo" type="org.magisoul.gcode.entity.GcDbConfigInfo"/>
        <typeAlias alias="GcJobInfo" type="org.magisoul.gcode.entity.GcJobInfo"/>
        <typeAlias alias="GcTableInfo" type="org.magisoul.gcode.entity.GcTableInfo"/>

        <typeAlias alias="QueryGcDbConfigInfoVo" type="org.magisoul.gcode.model.query.QueryGcDbConfigInfoVoVo"/>
        <typeAlias alias="QueryGcJobInfoVo" type="org.magisoul.gcode.model.query.QueryGcJobInfoVo"/>
        <typeAlias alias="QueryGcTableInfoVo" type="org.magisoul.gcode.model.query.QueryGcTableInfoVo"/>
    </typeAliases>
</configuration>